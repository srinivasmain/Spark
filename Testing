// Filter the rows based on the conditions
val resultDF = oradf.join(hivedf, Seq("tablename", "databasename"))
  .filter(col("colname").isin(oradfNotInHivedf: _*) || col("colname_1").isin(hivedfNotInOradf: _*))

// Select the desired columns
val finalDF = resultDF.select("csid", "csname", "colid", "colname", "tablename", "databasename")

// Show the final DataFrame
finalDF.show()
